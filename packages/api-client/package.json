{
  "name": "@gnndex/api-client",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "generate": "openapi-typescript ../../backend/openapi/openapi.v1.yaml -o src/gen/schema.ts",
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run generate && npm run build"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.0"
  },
  "devDependencies": {
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.7.2"
  }
}
